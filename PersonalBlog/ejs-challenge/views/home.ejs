<h1>Home</h1>
<p><%= homeStartingContent %></p>

<div>
    <% posts.forEach(function(post){ %>

        <h1 ><a style="color:black; text-decoration: none;"  href="/posts/<%= post.pTitle %>" ><%= post.pTitle %></h1></a>
